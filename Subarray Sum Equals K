class Solution {
    public int subarraySum(int[] nums, int k) {
        Map<Integer,Integer> m = new HashMap<>();
        int ps=0;
        int c=0;
        m.put(0,1);
        for(int i=0;i<nums.length;i++){
            ps += nums[i];
            int rem = ps - k;
            c += m.getOrDefault(rem,0);
            m.put(ps,m.getOrDefault(ps,0)+1);
        }
        return c;
    }
}
